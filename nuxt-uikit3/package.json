{
  "name": "@samk_dev/nuxt-uikit3",
  "version": "1.0.0-beta.6",
  "repository": "@samk-dev/nuxt-uikit3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "PORT=3666 nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "prerelease": "npm run prepack && changelogen --prerelease && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,vue,json}\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^0.5.5",
    "@nuxt/kit": "^3.5.1",
    "uikit": "^3.16.19"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.5.5",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.1",
    "@nuxt/test-utils": "^3.5.1",
    "@types/node": "^20.2.5",
    "@types/uikit": "^3.14.0",
    "changelogen": "^0.5.3",
    "eslint": "^8.41.0",
    "nuxt": "^3.5.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "vitest": "^0.31.1"
  }
}
